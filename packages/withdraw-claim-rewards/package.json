{
  "name": "@pooltogether/v5-autotasks-withdraw-claim-rewards",
  "version": "0.0.1",
  "description": "Autotask to automatically sweep prize claim rewards PoolTogether Hyperstructure (v5) prizes.",
  "author": "PoolTogether Inc.",
  "homepage": "https://github.com/pooltogether/v5-autotasks/packages/withdraw-claim-rewards/README.md",
  "license": "ISC",
  "type": "module",
  "main": "dist/handler.js",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pooltogether/v5-autotasks.git"
  },
  "private": true,
  "scripts": {
    "format": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --write \"**/*.{ts,js}\"",
    "yalcadd": "npx yalc add @pooltogether/v5-autotasks-library && npx yalc add @pooltogether/v5-utils-js",
    "build": "npm run yalcadd && tsup",
    "start": "npm run yalcadd && ts-node --esm --experimental-specifier-resolution=node ./src/cli.ts",
    "updatetask": "ts-node --esm --experimental-specifier-resolution=node ./script/updateTask.js",
    "update": "npm run yalcadd && npm run build && npm run updatetask"
  },
  "bugs": {
    "url": "https://github.com/pooltogether/v5-autotasks/issues"
  },
  "dependencies": {
    "@pooltogether/v5-autotasks-library": "file:.yalc/@pooltogether/v5-autotasks-library",
    "@pooltogether/v5-utils-js": "file:.yalc/@pooltogether/v5-utils-js",
    "configstore": "^6.0.0",
    "defender-relay-client": "1.39.0",
    "figlet": "^1.6.0",
    "inquirer": "^9.2.0",
    "tsdx": "^0.14.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.53",
    "aws-sdk": "^2.1364.0",
    "builtin-modules": "3.3.0",
    "defender-autotask-client": "1.37.0",
    "dotenv": "16.0.3",
    "es-main": "1.2.0",
    "ts-node": "10.9.1",
    "tsup": "^6.7.0",
    "yalc": "^1.0.0-pre.53"
  }
}
